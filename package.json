    "build": "babel src --out-dir lib",
    "watch:build": "babel src --out-dir lib --watch"
  },
  "devDependencies": {
    "eslint": "^1",
    "babel-eslint": "^4",
    "eslint-plugin-babel": "^3",
    "eslint-plugin-import": "^0",
